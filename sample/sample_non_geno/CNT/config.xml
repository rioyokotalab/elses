<?xml version="1.0" encoding="UTF-8"?>
<config name="CNT">

<system>
  <cluster structure="CNT.xml" />

  <boundary x="periodic" y="periodic" z="periodic" />

  <temperature unit="kelvin"> 273.15 </temperature>
  <element  name="C" model="Xu.1992"  filename="C.xml"> </element>
</system>

<calc mode="dynamics">
  <dynamics scheme="velocity verlet">
    <delta unit="fsec"> 1.00 </delta>
    <total unit="fsec"> 20.00 </total>
  </dynamics>
  <solver scheme="krylov">  </solver>
</calc>

<output>
  <restart  filename="CNT-restart.xml" interval="10" />
  <position filename="CNT-position.xyz" interval="4" />
</output>

</config>
