<?xml version="1.0" encoding="UTF-8"?>
<config name="si.diamond.structure">

<system>
  <cluster structure="Si512.xml" />
  <temperature unit="kelvin"> 273.15 </temperature>
  <boundary x="periodic" >  </boundary>
  <boundary y="periodic" >  </boundary>
  <boundary z="periodic" >  </boundary>
  <element  name="Si" model="NRL" filename="si_param.xml"> </element>
</system>

<calc mode="dynamics">
  <dynamics scheme="velocity verlet">
    <delta unit="fsec"> 1.00 </delta>
    <total unit="fsec"> 1.00 </total>
  </dynamics>
</calc>

<output>
  <restart  filename="si-restart.xml" interval="1" />
  <position filename="si-position.xyz" interval="1" />
</output>

</config>
